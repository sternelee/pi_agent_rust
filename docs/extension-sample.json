            "expect": {
              "content_contains": ["Invalid parameters. Provide exactly one mode."]
            }
          }
        ]
      },
      {
        "extension_id": "git-checkpoint",
        "features": ["event"],
        "scenarios": [
          {
            "id": "scn-git-checkpoint-001",
            "kind": "event",
            "event_name": "session_before_fork",
            "summary": "Offers to restore code state on fork and applies stash when user selects Yes (exec mocked)",
            "setup": {
              "mock_exec": [
                { "command": "git", "args": ["stash", "create"], "stdout": "deadbeef\n", "stderr": "", "code": 0 },
                { "command": "git", "args": ["stash", "apply", "deadbeef"], "stdout": "", "stderr": "", "code": 0 }
              ],
              "session_leaf_entry": { "id": "entry-1" }
            },
            "steps": [
              { "type": "emit_event", "event_name": "tool_result" },
              { "type": "emit_event", "event_name": "turn_start" },
              {
                "type": "emit_event",
                "event_name": "session_before_fork",
                "event": { "entryId": "entry-1" },
                "ctx": { "has_ui": true, "ui_responses": { "select": "Yes, restore code to that point" } }
              }
            ],
            "expect": {
              "exec_called": [["git", ["stash", "create"]], ["git", ["stash", "apply", "deadbeef"]]],
              "ui_notify_contains": ["Code restored to checkpoint"]
            }
          }
        ]
      }
