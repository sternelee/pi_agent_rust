    "items": [
      {
        "extension_id": "permission-gate",
        "features": ["event"],
        "scenarios": [
          {
            "id": "scn-permission-gate-001",
            "kind": "event",
            "event_name": "tool_call",
            "summary": "Blocks dangerous bash in non-interactive mode (no UI confirmation possible)",
            "input": {
              "event": { "toolName": "bash", "input": { "command": "rm -rf /tmp/boom" } },
              "ctx": { "has_ui": false }
            },
            "expect": {
              "block": true,
              "reason_contains": ["Dangerous command blocked"]
            }
          },
          {
            "id": "scn-permission-gate-002",
            "kind": "event",
            "event_name": "tool_call",
            "summary": "Prompts in UI and blocks when user selects No",
            "input": {
              "event": { "toolName": "bash", "input": { "command": "sudo echo hi" } },
              "ctx": { "has_ui": true, "ui_responses": { "select": "No" } }
            },
            "expect": {
              "block": true,
              "reason_contains": ["Blocked by user"]
            }
          }
        ]
      },
      {
        "extension_id": "protected-paths",
        "features": ["event"],
        "scenarios": [
          {
            "id": "scn-protected-paths-001",
            "kind": "event",
            "event_name": "tool_call",
            "summary": "Blocks write/edit operations targeting protected paths and emits a warning notification",
            "input": {
              "event": { "toolName": "write", "input": { "path": ".env", "content": "SECRET=1" } },
              "ctx": { "has_ui": true }
            },
            "expect": {
              "block": true,
              "reason_contains": ["protected"],
              "ui_notify_contains": ["Blocked write to protected path"]
            }
          }
        ]
      },
      {
        "extension_id": "todo",
        "features": ["tool", "command", "event"],
        "scenarios": [
          {
            "id": "scn-todo-001",
            "kind": "tool",
            "tool_name": "todo",
            "summary": "Adds a todo item and persists state via tool result details",
            "input": { "arguments": { "action": "add", "text": "Buy milk" } },
            "expect": {
              "content_contains": ["Added todo"],
              "details_contains_keys": ["action", "todos", "nextId"]
            }
          },
          {
            "id": "scn-todo-002",
            "kind": "command",
            "command_name": "todos",
            "summary": "Non-interactive /todos emits an error notification",
            "input": { "args": [], "ctx": { "has_ui": false } },
            "expect": {
              "ui_notify_contains": ["/todos requires interactive mode"]
            }
          },
          {
            "id": "scn-todo-003",
            "kind": "event",
            "event_name": "session_start",
            "summary": "Reconstructs todo state from session branch tool results on session_start",
            "setup": {
              "session_branch": [
                {
                  "type": "message",
                  "message": {
                    "role": "toolResult",
                    "toolName": "todo",
                    "details": {
                      "action": "add",
                      "todos": [{ "id": 1, "text": "Buy milk", "done": false }],
                      "nextId": 2
                    }
                  }
                }
              ]
            },
            "steps": [
              { "type": "emit_event", "event_name": "session_start" },
              { "type": "invoke_tool", "tool_name": "todo", "arguments": { "action": "list" } }
            ],
            "expect": {
              "final_content_contains": ["Buy milk"]
            }
          }
        ]
      },
      {
        "extension_id": "hello",
        "features": ["tool"],
        "scenarios": [
          {
            "id": "scn-hello-001",
            "kind": "tool",
            "tool_name": "hello",
            "summary": "Returns a greeting and details payload",
            "input": { "arguments": { "name": "World" } },
            "expect": {
              "content_contains": ["Hello, World!"],
              "details_exact": { "greeted": "World" }
            }
          }
        ]
      },
      {
        "extension_id": "antigravity-image-gen",
        "features": ["tool"],
        "scenarios": [
          {
            "id": "scn-antigravity-image-gen-001",
            "kind": "tool",
            "tool_name": "generate_image",
            "summary": "Errors when Google Antigravity OAuth credentials are missing (no network required)",
            "input": { "arguments": { "prompt": "a red square", "save": "none" } },
            "setup": { "mock_model_registry": { "google-antigravity": null } },
            "expect": {
              "is_error": true,
              "error_contains": ["Missing Google Antigravity OAuth credentials"]
            }
          },
          {
            "id": "scn-antigravity-image-gen-002",
            "kind": "tool",
            "tool_name": "generate_image",
            "summary": "Happy-path: mocked SSE stream returns text + image blocks with saveMode=none",
            "input": { "arguments": { "prompt": "a red square", "aspectRatio": "1:1", "save": "none" } },
            "setup": {
              "mock_model_registry": {
                "google-antigravity": "{\"token\":\"TOKEN\",\"projectId\":\"proj\"}"
              },
              "mock_http": { "mode": "vcr_or_stub", "note": "Stub streamGenerateContent SSE with a single image chunk" }
            },
            "expect": {
              "content_types": ["text", "image"],
              "details_contains_keys": ["provider", "model", "aspectRatio", "saveMode"]
            }
          }
        ]
      },
      {
        "extension_id": "plan-mode",
        "features": ["command", "event", "flag", "shortcut"],
        "scenarios": [
          {
            "id": "scn-plan-mode-001",
            "kind": "command",
            "command_name": "plan",
            "summary": "Enables plan mode and restricts tools to the read-only set",
            "input": { "args": "", "ctx": { "has_ui": true } },
            "expect": {
              "active_tools": ["read", "bash", "grep", "find", "ls", "questionnaire"],
              "ui_notify_contains": ["Plan mode enabled"]
            }
          },
          {
            "id": "scn-plan-mode-002",
            "kind": "event",
            "event_name": "tool_call",
            "summary": "Blocks non-allowlisted bash commands while plan mode is enabled",
            "setup": { "state": { "plan_mode_enabled": true } },
            "input": {
              "event": { "toolName": "bash", "input": { "command": "rm -rf /tmp/boom" } },
              "ctx": { "has_ui": false }
            },
            "expect": {
              "block": true,
              "reason_contains": ["Plan mode: command blocked"]
            }
          },
          {
            "id": "scn-plan-mode-003",
            "kind": "registration",
            "summary": "Verifies plan-mode registers flag, shortcut, and command",
            "expect": {
              "commands_contains": ["plan"],
              "event_hooks_contains": ["tool_call"]
            }
          }
        ]
      },
      {
        "extension_id": "status-line",
        "features": ["event"],
        "scenarios": [
          {
            "id": "scn-status-line-001",
            "kind": "event",
            "event_name": "turn_start",
            "summary": "Updates footer status across session_start → turn_start → turn_end",
            "input": { "ctx": { "has_ui": true } },
            "steps": [
              { "type": "emit_event", "event_name": "session_start" },
              { "type": "emit_event", "event_name": "turn_start" },
              { "type": "emit_event", "event_name": "turn_end" }
            ],
            "expect": {
              "ui_status_key": "status-demo",
              "ui_status_contains_sequence": ["Ready", "Turn 1", "complete"]
            }
          }
        ]
      },
      {
        "extension_id": "doom-overlay",
        "features": ["command"],
        "scenarios": [
          {
            "id": "scn-doom-overlay-001",
            "kind": "command",
            "command_name": "doom-overlay",
            "summary": "Non-interactive invocation emits an error notification and does not attempt UI overlay",
            "input": { "args": "", "ctx": { "has_ui": false } },
            "expect": {
              "ui_notify_contains": ["DOOM requires interactive mode"]
            }
          }
        ]
      },
      {
        "extension_id": "sandbox",
        "features": ["tool", "command", "event", "flag"],
        "scenarios": [
          {
            "id": "scn-sandbox-001",
            "kind": "tool",
            "summary": "Overrides the bash tool label to indicate sandboxing tier",
            "expect": {
              "tool_registered": { "name": "bash", "label": "bash (sandboxed)" }
            }
          },
          {
            "id": "scn-sandbox-002",
            "kind": "command",
            "command_name": "sandbox",
            "summary": "When sandbox is disabled, /sandbox shows an informational notification",
            "input": { "args": "", "ctx": { "has_ui": true } },
            "expect": {
              "ui_notify_contains": ["Sandbox is disabled"]
            }
          },
          {
            "id": "scn-sandbox-003",
            "kind": "event",
            "event_name": "session_start",
            "summary": "When --no-sandbox flag is set, disables sandboxing and warns the user",
            "setup": { "flags": { "no-sandbox": true } },
            "input": { "ctx": { "has_ui": true } },
            "expect": {
              "ui_notify_contains": ["Sandbox disabled via --no-sandbox"]
            }
          },
          {
            "id": "scn-sandbox-004",
            "kind": "registration",
            "summary": "Verifies sandbox registers a flag and event hooks",
            "expect": {
              "flags_contains": ["no-sandbox"],
              "event_hooks_contains": ["session_start"]
            }
          }
        ]
      },
      {
        "extension_id": "inline-bash",
        "features": ["event"],
        "scenarios": [
          {
            "id": "scn-inline-bash-001",
            "kind": "event",
            "event_name": "input",
            "summary": "Expands !{...} inline bash patterns and transforms prompt text",
            "input": { "event": { "text": "What's in !{pwd}?", "images": [] }, "ctx": { "has_ui": false } },
            "setup": {
              "mock_exec": [
                { "command": "bash", "args": ["-c", "pwd"], "stdout": "/repo\n", "stderr": "", "code": 0 }
              ]
            },
            "expect": {
              "action": "transform",
              "text_contains": ["/repo"]
            }
          }
        ]
      },
      {
        "extension_id": "dynamic-resources",
        "features": ["event"],
        "scenarios": [
          {
            "id": "scn-dynamic-resources-001",
            "kind": "event",
            "event_name": "resources_discover",
            "summary": "Returns skill/prompt/theme resources from the extension directory",
