      {
        "extension_id": "subagent",
        "features": ["tool"],
        "scenarios": [
          {
            "id": "scn-subagent-001",
            "kind": "tool",
            "tool_name": "subagent",
            "summary": "Rejects invalid parameter combinations (must specify exactly one mode)",
