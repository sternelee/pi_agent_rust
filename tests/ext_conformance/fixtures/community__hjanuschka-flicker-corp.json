{
  "extension": {
    "id": "community/hjanuschka-flicker-corp",
    "source": { "path": "community/hjanuschka-flicker-corp/flicker-corp.ts" }
  },
  "scenarios": [
    {
      "id": "scn-hjanuschka-flicker-corp-reg-001",
