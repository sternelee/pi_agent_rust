      "summary": "Invokes flicker-corp command",
      "command_name": "flicker-corp",
      "input": {
        "arguments": ""
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-hjanuschka-flicker-corp-neg-001",
      "kind": "command",
      "summary": "Invokes nonexistent command; expects error",
      "command_name": "flicker-invalid",
      "input": {
        "arguments": ""
      },
      "expect": {
        "is_error": true
      }
    }
  ]
}
