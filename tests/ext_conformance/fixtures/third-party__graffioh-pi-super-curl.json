{
  "extension": {
    "id": "third-party/graffioh-pi-super-curl",
    "source": {
      "path": "third-party/graffioh-pi-super-curl/index.ts"
    }
  },
  "scenarios": [
    {
      "id": "scn-graffioh-pi-super-curl-reg-001",
      "kind": "registration",
      "summary": "Verifies scurl command, event hooks registered for graffioh-pi-super-curl",
      "expect": {
        "commands_contains": [
          "scurl",
          "scurl-history",
          "scurl-log"
        ],
        "event_hooks_contains": [
          "session_start"
        ]
      }
    },
    {
      "id": "scn-graffioh-pi-super-curl-cmd-pos-001",
      "kind": "command",
      "summary": "Invokes scurl command",
      "command_name": "scurl",
      "input": {
        "arguments": ""
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-graffioh-pi-super-curl-cmd-neg-001",
      "kind": "command",
      "summary": "Invokes nonexistent command; expects error",
      "command_name": "scurl-invalid",
      "input": {
        "arguments": ""
      },
      "expect": {
        "is_error": true
      }
    },
    {
      "id": "scn-graffioh-pi-super-curl-evt-pos-001",
      "kind": "event",
      "summary": "Fires session_start event",
      "event_name": "session_start",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "no_error": true
      }
    }
  ]
}
