{
  "extension": {
    "id": "npm/pi-voice-of-god",
    "source": {
      "path": "npm/pi-voice-of-god/extensions/vog/index.ts"
    }
  },
  "scenarios": [
    {
      "id": "scn-npm-pi-voice-of-god-reg",
      "kind": "registration",
      "summary": "Validates command and event hook registrations",
      "expect": {
        "event_hooks_contains": [
          "before_agent_start",
          "session_start"
        ]
      }
    },
    {
      "id": "scn-npm-pi-voice-of-god-cmd-pos",
      "kind": "command",
      "summary": "Invokes vog command",
      "command_name": "vog",
      "input": {
        "arguments": ""
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-npm-pi-voice-of-god-evt-pos",
      "kind": "event",
      "summary": "Fires session_start event",
      "event_name": "session_start",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-npm-pi-voice-of-god-evt-neg",
      "kind": "event",
      "summary": "Fires unsupported event expecting graceful handling",
      "event_name": "unsupported_event",
      "input": {},
      "expect": {
        "is_error": true
      }
    }
  ]
}
