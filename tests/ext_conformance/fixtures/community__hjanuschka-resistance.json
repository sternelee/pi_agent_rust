{
  "extension": {
    "id": "community/hjanuschka-resistance",
    "source": { "path": "community/hjanuschka-resistance/resistance.ts" }
  },
  "scenarios": [
    {
      "id": "scn-hjanuschka-resistance-reg-001",
      "kind": "registration",
      "summary": "Verifies resistance command and event handlers are registered",
      "expect": {
        "commands_contains": ["resistance"],
        "event_hooks_contains": ["session_shutdown", "session_start"]
      }
    },
    {
      "id": "scn-hjanuschka-resistance-pos-001",
      "kind": "command",
      "summary": "Invokes resistance command",
      "command_name": "resistance",
      "input": {
        "arguments": ""
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-hjanuschka-resistance-pos-002",
      "kind": "event",
      "summary": "Fires session_start event",
      "event_name": "session_start",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-hjanuschka-resistance-neg-001",
      "kind": "event",
      "summary": "Fires unsupported event; expects error",
      "event_name": "unsupported_event",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "is_error": true
      }
    }
  ]
}
