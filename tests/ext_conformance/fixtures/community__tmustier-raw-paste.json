{
  "extension": {
    "id": "community/tmustier-raw-paste",
    "source": {
      "path": "community/tmustier-raw-paste/index.ts"
    }
  },
  "scenarios": [
    {
      "id": "scn-tmustier-raw-paste-reg-001",
      "kind": "registration",
      "summary": "Verifies paste command, event hooks registered for tmustier-raw-paste",
      "expect": {
        "commands_contains": [
          "paste"
        ],
        "event_hooks_contains": [
          "session_start"
        ]
      }
    },
    {
      "id": "scn-tmustier-raw-paste-cmd-pos-001",
      "kind": "command",
      "summary": "Invokes paste command",
      "command_name": "paste",
      "input": {
        "arguments": ""
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-tmustier-raw-paste-cmd-neg-001",
      "kind": "command",
      "summary": "Invokes nonexistent command; expects error",
      "command_name": "paste-invalid",
      "input": {
        "arguments": ""
      },
      "expect": {
        "is_error": true
      }
    },
    {
      "id": "scn-tmustier-raw-paste-evt-pos-001",
      "kind": "event",
      "summary": "Fires session_start event",
      "event_name": "session_start",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "no_error": true
      }
    }
  ]
}
