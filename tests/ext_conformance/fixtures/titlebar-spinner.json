{
  "extension": {
    "id": "titlebar-spinner",
    "source": { "path": "titlebar-spinner/titlebar-spinner.ts" }
  },
  "scenarios": [
    {
      "id": "scn-titlebar-spinner-reg-001",
      "kind": "registration",
      "summary": "Verifies agent_end, agent_start, and session_shutdown event handlers are registered",
      "expect": {
        "event_hooks_contains": ["agent_end", "agent_start", "session_shutdown"]
      }
    },
    {
      "id": "scn-titlebar-spinner-pos-001",
      "kind": "event",
      "summary": "Fires agent_start event to begin titlebar spinner animation",
      "event_name": "agent_start",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-titlebar-spinner-neg-001",
      "kind": "event",
      "summary": "Fires unsupported event; expects error",
      "event_name": "unsupported_event",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "is_error": true
      }
    }
  ]
}
