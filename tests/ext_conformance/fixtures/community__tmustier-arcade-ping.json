{
  "extension": {
    "id": "community/tmustier-arcade-ping",
    "source": {
      "path": "community/tmustier-arcade-ping/ping.ts"
    }
  },
  "scenarios": [
    {
      "id": "scn-tmustier-arcade-ping-reg-001",
      "kind": "registration",
      "summary": "Verifies ping command registered for tmustier-arcade-ping",
      "expect": {
        "commands_contains": [
          "ping"
        ]
      }
    },
    {
      "id": "scn-tmustier-arcade-ping-cmd-pos-001",
      "kind": "command",
      "summary": "Invokes ping command",
      "command_name": "ping",
      "input": {
        "arguments": ""
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-tmustier-arcade-ping-cmd-neg-001",
      "kind": "command",
      "summary": "Invokes nonexistent command; expects error",
      "command_name": "ping-invalid",
      "input": {
        "arguments": ""
      },
      "expect": {
        "is_error": true
      }
    }
  ]
}
