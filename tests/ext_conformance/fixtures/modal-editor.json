{
  "extension": {
    "id": "modal-editor",
    "source": { "path": "modal-editor/modal-editor.ts" }
  },
  "scenarios": [
    {
      "id": "scn-modal-editor-reg-001",
      "kind": "registration",
      "summary": "Verifies session_start event handler is registered",
      "expect": {
        "event_hooks_contains": ["session_start"]
      }
    },
    {
      "id": "scn-modal-editor-pos-001",
      "kind": "event",
      "summary": "Fires session_start event to initialize modal editor keybindings",
      "event_name": "session_start",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "no_error": true
      }
    },
    {
      "id": "scn-modal-editor-neg-001",
      "kind": "event",
      "summary": "Fires unsupported event; expects error",
      "event_name": "unsupported_event",
      "input": {
        "session_id": "test-session"
      },
      "expect": {
        "is_error": true
      }
    }
  ]
}
